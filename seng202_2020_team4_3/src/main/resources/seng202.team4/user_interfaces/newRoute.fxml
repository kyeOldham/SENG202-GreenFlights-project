<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<GridPane minHeight="440.0" prefWidth="720.0" stylesheets="@styles.css" vgap="7.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng202.team4.controller.NewRoute">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="15.0" prefHeight="50.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Text fx:id="errorText" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" visible="false" GridPane.halignment="CENTER" GridPane.rowIndex="10">
         <GridPane.margin>
            <Insets bottom="50.0" />
         </GridPane.margin>
      </Text>
      <JFXTextField fx:id="airlineField" focusColor="#298326" labelFloat="true" promptText="Airline IATA/ICAO" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets left="150.0" right="150.0" />
         </GridPane.margin>
      </JFXTextField>
      <JFXComboBox fx:id="depAirportCombo" editable="true" focusColor="#298326" labelFloat="true" prefHeight="26.0" prefWidth="418.0" promptText="Departure Airport IATA/ICAO" GridPane.halignment="CENTER" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets left="150.0" right="150.0" />
         </GridPane.margin>
      </JFXComboBox>
      <JFXComboBox fx:id="dstAirportCombo" editable="true" focusColor="#298326" labelFloat="true" prefHeight="26.0" prefWidth="418.0" promptText="Destination Airport IATA/ICAO" GridPane.halignment="CENTER" GridPane.rowIndex="3">
         <GridPane.margin>
            <Insets left="150.0" right="150.0" />
         </GridPane.margin>
      </JFXComboBox>
      <JFXCheckBox fx:id="codeshareField" prefHeight="30.0" prefWidth="111.0" text="Code Share" GridPane.halignment="CENTER" GridPane.rowIndex="4">
         <font>
            <Font size="13.0" />
         </font>
         <GridPane.margin>
            <Insets left="150.0" right="150.0" />
         </GridPane.margin>
      </JFXCheckBox>
      <JFXTextField fx:id="stopsField" focusColor="#298326" labelFloat="true" promptText="Number of Stops" GridPane.rowIndex="5">
         <GridPane.margin>
            <Insets left="150.0" right="150.0" />
         </GridPane.margin>
      </JFXTextField>
      <JFXTextField fx:id="equipmentField" focusColor="#298326" labelFloat="true" promptText="Plane Type(s)" GridPane.rowIndex="6">
         <GridPane.margin>
            <Insets left="150.0" right="150.0" />
         </GridPane.margin>
      </JFXTextField>
      <JFXComboBox fx:id="setComboBox" focusColor="#298326" labelFloat="true" prefHeight="26.0" prefWidth="204.0" promptText="Set" GridPane.halignment="CENTER" GridPane.rowIndex="8">
         <GridPane.margin>
            <Insets left="150.0" right="150.0" />
         </GridPane.margin>
      </JFXComboBox>
      <Label fx:id="editAddRoute" text="New Route Record" GridPane.halignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <GridPane GridPane.rowIndex="10">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <JFXButton onAction="#confirm" prefWidth="100.0" styleClass="button-upload" text="Confirm" GridPane.halignment="CENTER">
               <GridPane.margin>
                  <Insets bottom="50.0" />
               </GridPane.margin>
            </JFXButton>
            <JFXButton onAction="#cancel" prefWidth="100.0" style="-fx-background-color: #992626; -fx-text-fill: white;" text="Cancel" GridPane.columnIndex="1" GridPane.halignment="CENTER">
               <GridPane.margin>
                  <Insets bottom="50.0" />
               </GridPane.margin>
            </JFXButton>
         </children>
      </GridPane>
   </children>
</GridPane>
